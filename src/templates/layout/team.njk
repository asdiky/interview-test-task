{% block styles %}
	{{ super() }} 
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.2/tiny-slider.css">
		<link rel="stylesheet" href="styles/team.css" />
{% endblock %}
{% block scripts %}	
	{{ super() }} 
	<script src="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.2/min/tiny-slider.js"></script>
	<!--<script src="scripts/carousel.js"></script>-->
	<script>
	new tns({
	  "container": ".my-slider",
	  "responsive": {
	  	"320": {
      "items": 1
    },
	  	"640": {
      "items": 2
    },
		"960": {
       "items": 3
    }
  },
	  "rewind": true,
	  "swipeAngle": false,
	  "mouseDrag": true,
	   "slideBy": "page",
	  "speed": 400,
	  "navPosition": "bottom",
	  "controls": false,
	  "preventActionWhenRunning": true
	});
	</script>
{% endblock %}
{% block team %}
	<div id="team__fullwidth">
		<div id="team"> 
			<div id="team_tittle">
			<h1>Meet...the team</h1>
